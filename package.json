    "tree:src": "tree -I 'node_modules|.git|.next|dist|coverage' ./src",
    "tree:components": "tree -I 'node_modules|.git|.next|dist|coverage' ./src/components",
    "prepare": "simple-git-hooks",
    "type-check": "tsc --noEmit",
